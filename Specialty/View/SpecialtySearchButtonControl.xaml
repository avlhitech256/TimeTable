<UserControl x:Class="Specialty.View.SpecialtySearchButtonControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Specialty.View"
             xmlns:buttonsControl="clr-namespace:CommonControl.ButtonsControl;assembly=CommonControl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="180">
    <buttonsControl:SearchButtonControl x:Name="SearchButtonControl"
                                        ToolTipSearchButton="Поиск специальности"
                                        ToolTipClearButton="Очистить критерии поиска специальности"
                                        ToolTipAddButton="Создать новую специальность"
                                        ToolTipViewButton="Просмотр текущей записи специальности"
                                        ToolTipDeleteButton="Удалить текущую запись специальности">

    </buttonsControl:SearchButtonControl>
</UserControl>
