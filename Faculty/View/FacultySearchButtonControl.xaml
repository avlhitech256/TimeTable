<UserControl x:Class="Faculty.View.FacultySearchButtonControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Faculty.View"
             xmlns:buttonsControl="clr-namespace:CommonControl.ButtonsControl;assembly=CommonControl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="180">
    <buttonsControl:SearchButtonControl x:Name="SearchButtonControl"
                                        ToolTipSearchButton="Поиск факультета"
                                        ToolTipClearButton="Очистить критерии поиска факультетов"
                                        ToolTipAddButton="Создать новое факультет"
                                        ToolTipViewButton="Просмотр текущей записи факультета"
                                        ToolTipDeleteButton="Удалить текущую запись факультета">

    </buttonsControl:SearchButtonControl>
</UserControl>
