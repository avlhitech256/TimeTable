<UserControl x:Class="HighSchool.View.HighSchoolSearchButtonControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:buttonsControl="clr-namespace:CommonControl.ButtonsControl;assembly=CommonControl"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="150">
    <buttonsControl:SearchButtonControl x:Name="SearchButtonControl"
                                        ToolTipSearchButton="Поиск учебного заведения"
                                        ToolTipClearButton="Очистить критерии поиска учебных заведений"
                                        ToolTipAddButton="Создать новое учебное заведение"
                                        ToolTipViewButton="Просмотр текущей записи учебного заведения"
                                        ToolTipDeleteButton="Удалить текущую запись учебного заведения">
        
    </buttonsControl:SearchButtonControl>
</UserControl>
